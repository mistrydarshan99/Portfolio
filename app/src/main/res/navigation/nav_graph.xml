<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/nav_graph.xml"
            app:startDestination="@id/navigation_profile">

    <fragment android:id="@+id/navigation_profile"
              android:name="me.tumur.portfolio.view.fragments.ProfileFragment"
              android:label="@string/menu_profile"
              tools:layout="@layout/fragment_profile"/>

    <fragment android:id="@+id/navigation_portfolio"
              android:name="me.tumur.portfolio.view.fragments.PortfolioFragment"
              android:label="@string/menu_portfolio"
              tools:layout="@layout/fragment_portfolio"/>

    <fragment android:id="@+id/navigation_experience"
              android:name="me.tumur.portfolio.view.fragments.ExperienceFragment"
              android:label="@string/menu_experience"
              tools:layout="@layout/fragment_experience"/>

    <fragment android:id="@+id/navigation_settings"
              android:name="me.tumur.portfolio.view.fragments.SettingsFragment"
              android:label="@string/menu_settings"
              tools:layout="@layout/fragment_settings"/>
</navigation>